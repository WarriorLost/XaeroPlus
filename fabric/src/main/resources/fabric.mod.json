{
  "schemaVersion": 1,
  "id": "xaeroplus",
  "version": "${version}",
  "name": "XaeroPlus",
  "description": "Xaero's WorldMap and MiniMap Extra Features",
  "authors": [
    "rfresh2"
  ],
  "contact": {
    "homepage": "https://github.com/rfresh2/XaeroPlus",
    "sources": "https://github.com/rfresh2/XaeroPlus"
  },
  "license": "MIT",
  "icon": "assets/xaeroplus/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "xaeroplus.fabric.XaeroPlusFabric"
    ],
    "modmenu": [
      "xaeroplus.fabric.XaeroPlusModMenu"
    ]
  },
  "mixins": [
    "xaeroplus.mixins.json",
    "xaeroplus-fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.2",
    "minecraft": ">=1.21",
    "xaeroworldmap": "${worldmap_version}"
  },
  "custom": {
    "minimap_version": "${minimap_version}",
    "lithium:options": {
      "mixin.chunk.palette": false
    }
  }
}
